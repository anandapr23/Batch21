		"LS_SQL_PARAM_DB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "anand"
		},
		"NEW_Tumb_trigger_properties_pipeline_parameters_Window_Start_Time": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowStartTime"
		},
		"NEW_Tumb_trigger_properties_pipeline_parameters_Window_End_Time": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"event_trigger_properties_typeProperties_scope": {
			"type": "string",
