		{
			"name": "DF-Batch21_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_BLOB_KV_connectionString": {
						"value": "[parameters('LS_BLOB_KV_connectionString')]"
					},
					"LS_SQL_KV_properties_typeProperties_server": {
						"value": "[parameters('LS_SQL_KV_properties_typeProperties_server')]"
					},
					"LS_SQL_KV_properties_typeProperties_database": {
						"value": "[parameters('LS_SQL_KV_properties_typeProperties_database')]"
					},
					"LS_SQL_KV_properties_typeProperties_userName": {
						"value": "[parameters('LS_SQL_KV_properties_typeProperties_userName')]"
					},
					"NEW_Tumb_trigger_properties_pipeline_parameters_Window_Start_Time": {
						"value": "[parameters('NEW_Tumb_trigger_properties_pipeline_parameters_Window_Start_Time')]"
					},
					"NEW_Tumb_trigger_properties_pipeline_parameters_Window_End_Time": {
						"value": "[parameters('NEW_Tumb_trigger_properties_pipeline_parameters_Window_End_Time')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'DF-Batch21_ArmTemplate_0')]"
			]
		},
		{
			"name": "DF-Batch21_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
